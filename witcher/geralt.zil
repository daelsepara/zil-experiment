<OBJECT GERALT
    (DESC "Geralt of Rivia")
    (SYNONYM GERALT GWYNBLEIDD WOLF)
    (ADJECTIVE WHITE)
    (IN GLOBAL-OBJECTS)
    (ACTION WITCHER-F)
    (FLAGS NARTICLEBIT PERSONBIT)>

<OBJECT WITCHER
    (DESC "Witcher")
    (SYNONYM WITCHER WITCHERS)
    (IN GLOBAL-OBJECTS)
    (ACTION WITCHER-F)
    (FLAGS NARTICLEBIT PERSONBIT)>

<ROUTINE WITCHER-F ()
    <COND (<AND <VERB? EXAMINE LOOK-CLOSELY READ> <EQUAL? ,PRSO ,WITCHER ,GERALT>>
        <HMMM>
        <RTRUE>
    )>>
